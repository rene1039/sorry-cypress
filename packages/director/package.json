{
  "name": "@sorry-cypress/director",
  "version": "2.1.1",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agoldis/sorry-cypress.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/typings",
  "scripts": {
    "dev": "run-p dev:nodemon types:watch",
    "dev:nodemon": "nodemon",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "types:watch": "yarn build:types --watch --preserveWatchOutput",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "test": "jest",
    "lint": "eslint ./ --ext ts",
    "start": "node ./dist/",
    "clean": "rimraf ./dist",
    "release": "yarn version --no-git-tag-version"
  },
  "dependencies": {
    "@sorry-cypress/common": "1.0.0",
    "@sorry-cypress/logger": "1.0.0",
    "@sorry-cypress/mongo": "1.0.0",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.2",
    "clone-deep": "^4.0.1",
    "date-fns": "^2.21.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "graphql": "^15.3.0",
    "ibm-cos-sdk": "^1.8.0",
    "lodash": "^4.17.21",
    "md5": "^2.2.1",
    "minio": "^7.0.15",
    "pm2": "^5.1.2",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.20",
    "tunnel": "^0.0.6"
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/clone-deep": "^4.0.1",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/minio": "^7.0.5",
    "@types/node": "^14.11.2",
    "@types/semver": "^7.3.8",
    "@types/uuid": "^3.4.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3"
  }
}
